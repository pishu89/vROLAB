<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<ns2:workflow xmlns:ns2="http://vmware.com/vco/workflow" root-name="item1" object-name="workflow:name=generic" id="e3030e59-06de-47fe-84ec-45b7c59df665" editor-version="2.0" version="0.0.0" api-version="6.0.0" restartMode="1" resumeFromFailedMode="0">
    <display-name>Generate Hostname with Variables</display-name>
    <description>This workflow illustrates how to generate a host name using variables</description>
    <position y="50.0" x="100.0"/>
    <input>
        <param name="nextNum" type="string">
            <description>Machine number</description>
        </param>
    </input>
    <output/>
    <attrib name="cluster" type="string">
        <value encoded="n">CL3</value>
        <description>Name of the cluster used in the host name</description>
    </attrib>
    <attrib name="businessGroup" type="string">
        <value encoded="n">QA</value>
        <description>Name of the business group used in the host name</description>
    </attrib>
    <attrib name="operatingSystem" type="string">
        <value encoded="n">RH7</value>
        <description>Name of the operating system used in the host name</description>
    </attrib>
    <attrib name="hostname" type="string">
        <value encoded="n"></value>
        <description>Generated host name</description>
    </attrib>
    <workflow-item name="item0" type="end" end-mode="0" comparator="0">
        <in-binding/>
        <out-binding/>
        <position y="50.0" x="410.0"/>
    </workflow-item>
    <workflow-item name="item1" out-name="item2" type="task" comparator="0">
        <display-name>Generate Hostname</display-name>
        <script encoded="false">var hostname = cluster + "-" + businessGroup + "-" + operatingSystem + "-" + nextNum;</script>
        <in-binding>
            <bind name="cluster" type="string" export-name="cluster"/>
            <bind name="businessGroup" type="string" export-name="businessGroup"/>
            <bind name="operatingSystem" type="string" export-name="operatingSystem"/>
            <bind name="nextNum" type="string" export-name="nextNum"/>
        </in-binding>
        <out-binding>
            <bind name="hostname" type="string" export-name="hostname"/>
        </out-binding>
        <description>Simple task with custom script capability.</description>
        <position y="60.0" x="170.0"/>
    </workflow-item>
    <workflow-item name="item2" out-name="item0" type="task" prototype-id="system-log" interaction="l" comparator="0">
        <display-name>Display Hostname</display-name>
        <script encoded="false">//Auto-generated script
System.log(text);</script>
        <in-binding>
            <bind name="text" type="string" export-name="hostname">
                <description>The text to log</description>
            </bind>
        </in-binding>
        <out-binding/>
        <description>Log the input text to the console log with the level "log".</description>
        <position y="60.0" x="275.0"/>
    </workflow-item>
    <presentation/>
</ns2:workflow>
