<?xml version='1.0' encoding='UTF-8'?>
<workflow xmlns="http://vmware.com/vco/workflow" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://vmware.com/vco/workflow http://vmware.com/vco/workflow/Workflow-v4.xsd" root-name="item1" object-name="workflow:name=generic" id="ef816cb9-7a0b-45e8-b53a-5fa1c6de3b65" version="0.0.0" api-version="6.0.0" allowed-operations="evf" editor-version="2.0" restartMode="1" resumeFromFailedMode="0">
  <display-name><![CDATA[Create hostname with Inputs]]></display-name>
  <position y="50.0" x="100.0"/>
  <input>
    <param name="nextNum" type="number">
      <description><![CDATA[Machine Number]]></description>
    </param>
    <param name="cluster" type="string">
      <description><![CDATA[Cluster for hostname]]></description>
    </param>
    <param name="businessGroup" type="string">
      <description><![CDATA[Business Group for hostname]]></description>
    </param>
    <param name="operatingSytem" type="string">
      <description><![CDATA[OS for hostname]]></description>
    </param>
  </input>
  <output>
    <param name="outHostname" type="string"/>
  </output>
  <attrib name="hostname" type="string" read-only="false">
    <value encoded="n"><![CDATA[]]></value>
    <description><![CDATA[Calculated hostname]]></description>
  </attrib>
  <workflow-item name="item0" type="end" end-mode="0">
    <position y="50.0" x="505.0"/>
  </workflow-item>
  <workflow-item name="item1" out-name="item2" type="task">
    <display-name><![CDATA[Create Hostname]]></display-name>
    <script encoded="false"><![CDATA[var hostname = cluster + "-" + businessGroup + "-" + operatingSystem + "-" + nextNum;]]></script>
    <in-binding>
      <bind name="cluster" type="string" export-name="cluster"/>
      <bind name="operatingSystem" type="string" export-name="operatingSytem"/>
      <bind name="nextNum" type="number" export-name="nextNum"/>
      <bind name="businessGroup" type="string" export-name="businessGroup"/>
    </in-binding>
    <out-binding>
      <bind name="hostname" type="string" export-name="hostname"/>
    </out-binding>
    <description><![CDATA[Calculate the hostname]]></description>
    <position y="60.0" x="143.0"/>
  </workflow-item>
  <workflow-item name="item2" prototype-id="system-log" out-name="item3" type="task" interaction="l">
    <display-name><![CDATA[Display Hostname]]></display-name>
    <script encoded="false"><![CDATA[//Auto-generated script
System.log(text);]]></script>
    <in-binding>
      <bind name="text" type="string" export-name="hostname">
        <description><![CDATA[The text to log]]></description>
      </bind>
    </in-binding>
    <out-binding/>
    <description><![CDATA[Log the hostname]]></description>
    <position y="60.0" x="265.0"/>
  </workflow-item>
  <workflow-item name="item3" out-name="item0" type="task">
    <display-name><![CDATA[Output Hostname]]></display-name>
    <script encoded="false"><![CDATA[outHostname = hostname;]]></script>
    <in-binding>
