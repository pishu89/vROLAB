<?xml version='1.0' encoding='UTF-8'?>
<workflow xmlns="http://vmware.com/vco/workflow" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://vmware.com/vco/workflow http://vmware.com/vco/workflow/Workflow-v4.xsd" root-name="item1" object-name="workflow:name=generic" id="ce0ffb3e-4518-44f0-a7f2-498d68200867" version="0.0.0" api-version="6.0.0" allowed-operations="vfe" editor-version="2.0" restartMode="1" resumeFromFailedMode="0">
  <display-name><![CDATA[Generate Hostname with Variables]]></display-name>
  <description><![CDATA[This workflow illustrates how to generate a host name using variables]]></description>
  <position y="80.0" x="80.0"/>
  <input>
    <param name="nextNum" type="number">
      <description><![CDATA[Machine Number]]></description>
    </param>
  </input>
  <attrib name="cluster" type="string" read-only="false">
    <value encoded="n"><![CDATA[CL3]]></value>
    <description><![CDATA[Name of cluster used in hostname]]></description>
  </attrib>
  <attrib name="businessGroup" type="string" read-only="false">
    <value encoded="n"><![CDATA[QA]]></value>
    <description><![CDATA[Name of business group used in hostname]]></description>
  </attrib>
  <attrib name="operatingSystem" type="string" read-only="false">
    <value encoded="n"><![CDATA[RH7]]></value>
    <description><![CDATA[Name of operating system used in hostname]]></description>
  </attrib>
  <attrib name="hostname" type="string" read-only="false">
    <value encoded="n"><![CDATA[]]></value>
    <description><![CDATA[Calculated hostname]]></description>
  </attrib>
  <workflow-item name="item0" type="end" end-mode="0">
    <position y="80.0" x="470.0"/>
  </workflow-item>
  <workflow-item name="item1" out-name="item2" type="task">
    <display-name><![CDATA[Generate Hostname]]></display-name>
    <script encoded="false"><![CDATA[var hostname = cluster + "-" + businessGroup + "-" + operatingSystem + "-" + nextNum;]]></script>
    <in-binding>
      <bind name="cluster" type="string" export-name="cluster"/>
      <bind name="operatingSystem" type="string" export-name="operatingSystem"/>
      <bind name="nextNum" type="number" export-name="nextNum"/>
      <bind name="businessGroup" type="string" export-name="businessGroup"/>
    </in-binding>
    <out-binding>
      <bind name="hostname" type="string" export-name="hostname"/>
    </out-binding>
    <description><![CDATA[Calculate the hostname]]></description>
    <position y="90.0" x="170.0"/>
  </workflow-item>
  <workflow-item name="item2" prototype-id="system-log" out-name="item0" type="task" interaction="l">
    <display-name><![CDATA[Display Hostname]]></display-name>
    <script encoded="false"><![CDATA[//Auto-generated script
System.log(text);]]></script>
    <in-binding>
      <bind name="text" type="string" export-name="hostname">
        <description><![CDATA[The text to log]]></description>
      </bind>
    </in-binding>
    <out-binding/>
    <description><![CDATA[Log the hostname]]></description>
    <position y="90.0" x="300.0"/>
  </workflow-item>
  <presentation/>
</workflow>