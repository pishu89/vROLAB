    <workflow-item name="item1" out-name="item2" type="link" linked-workflow-id="BD8080808080808080808080808080808EC380800122528313869552e41805bb1" comparator="0">
        <display-name>Create first VM folder</display-name>
        <script encoded="false"></script>
        <in-binding>
            <bind name="parentFolder" type="VC:VmFolder" export-name="parentFolder">
                <description>Parent folder</description>
